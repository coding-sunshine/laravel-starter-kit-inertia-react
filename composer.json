{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "nunomaduro/laravel-starter-kit-inertia-react",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "repositories": [
        {
            "name": "filament",
            "type": "composer",
            "url": "https://packages.filamentphp.com/composer"
        }
    ],
    "require": {
        "php": "^8.4.0",
        "a909m/filament-statefusion": "^2.3.0",
        "alizharb/filament-activity-log": "^1.3.1",
        "askedio/laravel-soft-cascade": "^12.0",
        "dedoc/scramble": "^0.13.12",
        "essa/api-tool-kit": "^2.2",
        "filament/filament": "^5.2.0",
        "filament/spatie-laravel-settings-plugin": "^5.2",
        "inertiajs/inertia-laravel": "^2.0.19",
        "laravel/ai": "^0.1.3",
        "laravel/fortify": "^1.34.1",
        "laravel/framework": "^12.50.0",
        "laravel/mcp": "^0.5.5",
        "laravel/pennant": "^1.19",
        "laravel/sanctum": "^4.3",
        "laravel/scout": "^10.23",
        "laravel/slack-notification-channel": "^3.7",
        "laravel/tinker": "^2.11.0",
        "laravel/wayfinder": "^0.1.13",
        "nunomaduro/essentials": "^1.0.1",
        "pgvector/pgvector": "^0.2.2",
        "prism-php/prism": "^0.99.19",
        "prism-php/relay": "^1.6",
        "pxlrbt/filament-excel": "^3.4",
        "saloonphp/saloon": "^3.14.2",
        "sentry/sentry-laravel": "^4.20.1",
        "spatie/eloquent-sortable": "^4.5.2",
        "spatie/laravel-activitylog": "^4.11",
        "spatie/laravel-analytics": "^5.6.1",
        "spatie/laravel-backup": "^9.4",
        "spatie/laravel-cookie-consent": "^3.3.3",
        "spatie/laravel-csp": "^3.22",
        "spatie/laravel-data": "^4.19.1",
        "spatie/laravel-honeypot": "^4.6.2",
        "spatie/laravel-medialibrary": "^11.17.10",
        "spatie/laravel-model-flags": "^1.4.1",
        "spatie/laravel-model-states": "^2.12.2",
        "spatie/laravel-pdf": "^1.9",
        "spatie/laravel-permission": "^6.24",
        "spatie/laravel-personal-data-export": "^4.3.1",
        "spatie/laravel-query-builder": "^6.4.1",
        "spatie/laravel-responsecache": "^7.7.2",
        "spatie/laravel-schemaless-attributes": "^2.5.1",
        "spatie/laravel-settings": "^3.6",
        "spatie/laravel-sitemap": "^7.3.8",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/laravel-tags": "^4.10.1",
        "stechstudio/filament-impersonate": "^4.1.3",
        "stephenjude/filament-feature-flags": "^5.0",
        "typesense/typesense-php": "^6.0",
        "wildside/userstamps": "^3.1.1"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.1.9",
        "fakerphp/faker": "^1.24.1",
        "filament/blueprint": "^2.1",
        "larastan/larastan": "^3.9.2",
        "laravel/boost": "^2.1.1",
        "laravel/pail": "^1.2.5",
        "laravel/pint": "^1.27.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "pestphp/pest": "^4.3.2",
        "pestphp/pest-plugin-browser": "^4.2.1",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "pestphp/pest-plugin-type-coverage": "^4.0.3",
        "rector/rector": "^2.3.6",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "bun install",
            "bun run build"
        ],
        "setup-blueprint": "bash scripts/setup-filament-blueprint.sh",
        "optimize-production": [
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan filament:cache-components"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "bunx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"bun run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan docs:sync || true",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan clear",
            "@update:requirements",
            "@php artisan boost:update --ansi || true",
            "@php artisan docs:sync || true"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "update:requirements": [
            "composer bump",
            "bunx npm-check-updates -u"
        ],
        "lint": [
            "rector",
            "pint --parallel",
            "bun run lint"
        ],
        "test:type-coverage": "pest --type-coverage --min=100",
        "test:lint": [
            "pint --parallel --test",
            "rector --dry-run",
            "bun run test:lint"
        ],
        "test:unit": "XDEBUG_MODE=\"coverage\" pest --parallel --coverage --exactly=100.0",
        "test:types": [
            "phpstan",
            "bun run test:types"
        ],
        "test": [
            "@test:type-coverage",
            "@test:unit",
            "@test:lint",
            "@test:types"
        ],
        "docs:check": "php artisan docs:sync --check",
        "docs:sync": "php artisan docs:sync",
        "docs:generate": "php artisan docs:sync --generate"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
