        "two_factor_recovery_codes": {
            "type": "text",
            "nullable": true,
            "default": null
        },
        "two_factor_confirmed_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        },
        "remember_token": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "created_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        },
        "updated_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
