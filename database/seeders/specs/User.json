            "default": null
        },
        "name": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "email": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "email_verified_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        },
        "password": {
            "type": "varchar",
