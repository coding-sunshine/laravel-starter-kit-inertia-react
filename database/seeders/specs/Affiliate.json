            "nullable": true,
            "default": null
        },
        "payment_email": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "payment_method": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "payment_details": {
            "type": "text",
            "nullable": true,
            "default": null
        },
        "total_earnings": {
            "type": "integer",
            "nullable": true,
