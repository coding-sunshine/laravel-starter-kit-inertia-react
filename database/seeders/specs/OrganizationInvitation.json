            "model": "Organization"
        },
        "inviter": {
            "type": "belongsTo",
            "model": "Inviter"
        }
    },
    "value_hints": {
        "expires_at": {
            "type": "datetime",
            "example": "2024-01-01 00:00:00"
        },
        "accepted_at": {
            "type": "datetime",
            "example": "2024-01-01 00:00:00"
        },
        "email": {
            "type": "email",
            "example": "user@example.com"
        }
    },
    "scenarios": []
}