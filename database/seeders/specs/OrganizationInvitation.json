{
    "model": "OrganizationInvitation",
    "table": "organization_invitations",
    "fields": {
        "id": {
            "type": "integer",
            "nullable": true,
            "default": null
        },
        "organization_id": {
            "type": "integer",
            "nullable": true,
            "default": null
        },
        "email": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "role": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "status": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "token": {
            "type": "varchar",
            "nullable": true,
            "default": null
        },
        "invited_by": {
            "type": "integer",
            "nullable": true,
            "default": null
        },
        "expires_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        },
        "accepted_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        },
        "created_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        },
        "updated_at": {
            "type": "datetime",
            "nullable": true,
            "default": null
        }
    },
    "relationships": {
